<%= form_for(@track) do |f| %>
  <%= render 'shared/error_messages', object: @track %>
  <div class = "field">
    <%= f.label :title %>
    <%= f.text_field :title %>
  <div>

  <div class = "field">
    <%= f.label :track_file %>
    <%= f.file_field :track_file %>
  <div>
  <%= f.submit "Post", class: "button" %>
<% end %>

